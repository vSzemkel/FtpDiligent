<Window x:Class="FtpDiligent.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FtpDiligent"
        mc:Ignorable="d"
        Title="FtpDiligent" Height="800" Width="1000" ResizeMode="NoResize"
        Icon="/Images/ftpgetw.ico"
        Closed="Window_Closed">
    <TabControl Name="tcMain">
        <TabItem Name="tabSterowanie" Header="Sterownie" />
        <TabItem Name="tabSerwery" Header="Serwery" />
        <TabItem Name="tabSerweryDetails" Header="Dane serwera" Visibility="Collapsed"/>
        <TabItem Name="tabHarmonogramy" Header="Harmonogramy" />
        <TabItem Name="tabHarmonogramyDetails" Header="Dane harmonogramu" Visibility="Collapsed" />
    </TabControl>
</Window>
